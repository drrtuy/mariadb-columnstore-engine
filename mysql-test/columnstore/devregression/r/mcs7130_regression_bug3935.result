select n_nationkey, n_name into outfile '/tmp/bug3935_outfile.txt'
FIELDS TERMINATED BY '|' LINES TERMINATED BY '/n' from nation;
select n_nationkey, n_name into outfile '/tmp/bug3935_outfile.txt'
